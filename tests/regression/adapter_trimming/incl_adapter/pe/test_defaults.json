{
  "description": "Adapter trimming of PE reads containing adapters using 1-3 threads",
  "exhaustive": true,
  "arguments": [
    "--out-prefix",
    "your_output"
  ],
  "files": {
    "input_1": [
      "input_1.fastq"
    ],
    "input_2": [
      "input_2.fastq"
