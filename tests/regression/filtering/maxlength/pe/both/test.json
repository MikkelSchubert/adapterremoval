{
  "description": "Filtering reads by maximum length excluding both mate reads",
  "arguments": [
    "--out-prefix",
    "your_output",
    "--out-discarded",
    "your_output.discarded.fastq",
    "--max-length",
    "49"
  ],
  "files": {
    "input_1": [
      "input_1.fastq"
    ],
    "input_2": [
      "input_2.fastq"
    ],
    "output": [
      "your_output.discarded.fastq",
      "your_output.r1.fastq.gz",
      "your_output.r2.fastq.gz",
      "your_output.singleton.fastq.gz"
    ],
    "json": [
      "your_output.json"
    ],
    "html": [
      "your_output.html"
    ]
  }
}