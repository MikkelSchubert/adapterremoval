{
  "arguments": [
    "--out-prefix",
    "your_output",
    "--out-discarded",
    "your_output.discarded.fastq",
    "--trim-strategy",
    "window",
    "--trim-windows",
    "4",
    "--trim-min-quality",
    "20",
    "--trim-ns"
  ],
  "files": {
    "input_1": [
      "input_1.fastq"
    ],
    "input_2": [
      "input_2.fastq"
    ],
    "output": [
      "your_output.discarded.fastq",
      "your_output.r1.fastq.gz",
      "your_output.r2.fastq.gz",
      "your_output.singleton.fastq.gz"
    ],
    "ignore": [
      "your_output.json",
      "your_output.html"
    ]
  }
}