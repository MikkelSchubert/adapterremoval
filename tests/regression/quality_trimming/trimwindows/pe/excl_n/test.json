{
  "arguments": [
    "--basename",
    "your_output",
    "--out-discarded",
    "your_output.discarded.fastq",
    "--trim-strategy",
    "window",
    "--trimwindows",
    "4",
    "--minquality",
    "20"
  ],
  "files": {
    "input_1": [
      "input_1.fastq"
    ],
    "input_2": [
      "input_2.fastq"
    ],
    "output": [
      "your_output.discarded.fastq",
      "your_output.r1.fastq",
      "your_output.r2.fastq",
      "your_output.singleton.fastq"
    ],
    "ignore": [
      "your_output.json",
      "your_output.html"
    ]
  }
}