{
  "description": "Trimming with default error rate. Should not trim additional bases compared to --trim-mott-quality 13",
  "arguments": [
    "--quality-trimming",
    "mott",
    "--trim-mott-quality",
    "19",
    "--out-prefix",
    "output_19"
  ],
  "files": {
    "input_1": [
      "input_1.fastq"
    ],
    "input_2": [
      "input_2.fastq"
    ],
    "output": [
      "output_19.r1.fastq.gz",
      "output_19.r2.fastq.gz"
    ],
    "ignore": [
      "output_19.singleton.fastq.gz",
      "output_19.json",
      "output_19.html"
    ]
  }
}