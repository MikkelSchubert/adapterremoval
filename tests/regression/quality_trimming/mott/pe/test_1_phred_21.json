{
  "description": "Trimming with default error rate. Expects same results as --trim-mott-quality 20",
  "arguments": [
    "--quality-trimming",
    "mott",
    "--trim-mott-quality",
    "21",
    "--out-prefix",
    "output_20"
  ],
  "files": {
    "input_1": [
      "input_1.fastq"
    ],
    "input_2": [
      "input_2.fastq"
    ],
    "output": [
      "output_20.r1.fastq.gz",
      "output_20.r2.fastq.gz"
    ],
    "ignore": [
      "output_20.singleton.fastq.gz",
      "output_20.json",
      "output_20.html"
    ]
  }
}