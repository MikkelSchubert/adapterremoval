{
  "description": "Trimming with default error rate",
  "arguments": [
    "--quality-trimming",
    "mott",
    "--out-prefix",
    "output_defaults"
  ],
  "files": {
    "input_1": [
      "input_1.fastq"
    ],
    "input_2": [
      "input_2.fastq"
    ],
    "output": [
      "output_defaults.r1.fastq.gz",
      "output_defaults.r2.fastq.gz"
    ],
    "ignore": [
      "output_defaults.singleton.fastq.gz",
      "output_defaults.json",
      "output_defaults.html"
    ]
  }
}
