{
  "description": "Trimming list of adapters containing multiple adapter pairs",
  "arguments": [
    "--basename",
    "your_output",
    "--adapter-list",
    "adapters.txt"
  ],
  "files": {
    "input_1": [
      "reads_1.fastq"
    ],
    "input_2": [
      "reads_2.fastq"
    ],
    "adapters": [
      "adapters.txt"
    ],
    "output": [
      "your_output.r1.fastq.gz",
      "your_output.r2.fastq.gz",
      "your_output.singleton.fastq.gz"
    ],
    "json": [
      "your_output.json"
    ],
    "html": [
      "your_output.html"
    ]
  }
}