{
  "arguments": [
    "--basename",
    "your_output",
    "--prefix-read1",
    "R1_",
    "--prefix-read2",
    "R2_",
    "--demultiplex-only"
  ],
  "files": {
    "input_1": [
      "input_1.fastq"
    ],
    "input_2": [
      "input_2.fastq"
    ],
    "barcodes": [
      "barcodes.txt"
    ],
    "output": [
      "your_output.sample_1.r1.fastq",
      "your_output.sample_1.r2.fastq",
      "your_output.unidentified.r1.fastq",
      "your_output.unidentified.r2.fastq"
    ],
    "ignore": [
      "your_output.json",
      "your_output.html"
    ]
  }
}