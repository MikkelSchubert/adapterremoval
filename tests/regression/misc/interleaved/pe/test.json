{
  "exhaustive": false,
  "arguments": [
    "--basename",
    "your_output",
    "--interleaved-output",
    "--out-file1",
    "interleaved.fastq"
  ],
  "files": {
    "input_1": [
      "input_1.fastq"
    ],
    "input_2": [
      "input_2.fastq"
